!########################
!	Xcursor
!
Xcursor*theme:   Obsidian
MRxvt*keysym.Home: \033[1~
MRxvt*keysym.End: \033[4~
Emacs.FontBackend: xft
Emacs.font: Consolas-9
Xft.dpi: 96
Xft.hinting: slight


!! BEGIN urlLauncher settings !!
urxvt*perl-lib:            /usr/lib/urxvt/perl/
urxvt*perl-ext-common:        default,matcher
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*matcher.button:        1
!! END urlLauncher settings !!

!########################
!	Urxvt
!

urxvt*depth:       24
urxvt*inheritPixmap: true
urxvt*termName: rxvt
urxvt*scrollBar: false
urxvt*scrollstyle: plain
urxvt*mouseWheelScrollPage: false
urxvt*saveLines: 2767
urxvt*cursorBlink: true
urxvt*shading: 10
#urxvt.font: -*-proggyclean-*-*-*-*-*-*-*-*-*-*-*-*
urxvt.font: -*-terminus-*-*-*-*-12-*-*-*-*-*-*-*
urxvt*transparent: true
urxvt*tintColor: #000000
!urxvt*borderLess: true
!urxvt*externalBorder:0
!urxvt*internalBorder:8
!urxvt*borderColor:0
!urxvt.skipBuiltinGlyphs:true
urxvt.intensityStyles:    true
urxvt*background: #000000
urxvt*foreground: #C4C4C4

