#Stop
"remoot stop"
    m:0x0 + c:174
    XF86AudioStop 

#Back
"remoot prev"
    m:0x0 + c:173
    XF86AudioPrev 

#Play/Pause
"remoot playpause"
    m:0x0 + c:172
    XF86AudioPlay 

#Next
"remoot next"
    m:0x0 + c:171
    XF86AudioNext 

#Mute
"Command"
    m:0x0 + c:121
    XF86AudioMute 

#VolDown
"ossmix vmix0-outvol -- -1"
    m:0x0 + c:122
    XF86AudioLowerVolume 

#VolUp
"ossmix vmix0-outvol -- +1"
    m:0x0 + c:123
    XF86AudioRaiseVolume 

#ScreenDown
""
    m:0x0 + c:101
    XF86KbdBrightnessDown 

#ScreenUp
""
    m:0x0 + c:233
    XF86MonBrightnessUp 

